package clients;

import factories.TextComponentFactory;
import textComponents.Document;
import textComponents.Heading;
import textComponents.Paragraph;

public class ServiceHost
{

	public static void main(String[] args)
	{
		Heading heading = new Heading("Rubrik 1");
		Paragraph paragraph = new Paragraph("Detta Ã¤r en enkel text i stycket.");

		Document document = TextComponentFactory.createDocument();
		document.addComponent(heading);
		document.addComponent(paragraph);

		// Rendera hela dokumentet
		document.render();

	}

}
