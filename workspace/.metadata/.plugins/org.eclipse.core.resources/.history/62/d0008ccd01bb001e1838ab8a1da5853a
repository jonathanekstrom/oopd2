package clients;

import elements.Document;
import tjofraes.*;

public class Client
{
	public static void main(String[] args)
	{
		AbstractElementFactory elementFactory = new ConcreteElementFactory();
		
		IElement elementOne = elementFactory.createElement(ElementTypes.HEADING);
		IElement elementTwo = elementFactory.createElement(ElementTypes.PARAGRAPH);
		IElement e3 = elementFactory.createElement(ElementTypes.TABLE);
		
		Document d = new Document();
		
		d.addElement(elementOne);
		d.addElement(elementTwo);
		d.addElement(e3);
		
		d.render();
	}
}
