package textDocBuilder.factories;

import textDocBuilder.domain.Font;
import textDocBuilder.enums.FontSizes;
import textDocBuilder.enums.FontTypes;

public abstract class AbstractFontFactory
{
	public Font createFont(Font font)
	{
		Font f = getFont(font);
		return f;
	}

	protected abstract FontTypes getFontType();
	protected abstract FontSizes getFontSize();
}
