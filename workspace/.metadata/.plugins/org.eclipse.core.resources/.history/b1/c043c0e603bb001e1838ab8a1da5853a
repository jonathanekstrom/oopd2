package clients;

import interfaces.ElementTypes;
import interfaces.IElement;
import interfaces.IIterator;
import tjofraes.*;

public class Client
{
	public static void main(String[] args)
	{
		AbstractElementFactory elementFactory = new ConcreteElementFactory();
		
		IElement heading = elementFactory.createElement(ElementTypes.HEADING);
		IElement paragraph = elementFactory.createElement(ElementTypes.PARAGRAPH);
		IElement table = elementFactory.createElement(ElementTypes.TABLE);
		
		Document d = new Document();
		
		d.addElement(heading);
		d.addElement(paragraph);
		d.addElement(table);
		
		IIterator iterator = d.getIterator();
	}
}
