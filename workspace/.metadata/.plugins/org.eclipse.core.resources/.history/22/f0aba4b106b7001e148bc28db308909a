package models;

public class Square
{
	private boolean isOdd;
	private String color;

	public Square(boolean isOdd)
	{
		this.isOdd = isOdd;
	}

	public boolean isOdd()
	{
		return isOdd;
	}

	public String getColor()
	{
		return color;
	}

	public void setColor(String color)
	{
		this.color = color;
	}
}
