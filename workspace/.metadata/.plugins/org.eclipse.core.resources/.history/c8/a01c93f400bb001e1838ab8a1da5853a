package clients;

import tjofraes.*;

public class ServiceHost
{
	public static void main(String[] args)
	{
		AbstractElementFactory creator = new ConcreteElementFactory();
		
		IElement e1 = creator.createElement(ElementTypes.HEADING);
		IElement e2 = creator.createElement(ElementTypes.PARAGRAPH);
		IElement e3 = creator.createElement(ElementTypes.TABLE);
		
		Document d = new Document();
		
		d.addComponent(e1);
		d.addComponent(e2);
		d.addComponent(e3);
		
		d.render();
	}
}
